  "repository": "StylishThemes/GitHub-Sticky-Sidebar",
  "license": "CC-BY-SA-4.0",
  "main": "github-sticky-sidebar.user.css",
  "devDependencies": {
    "updates": "^6.2.1",
    "versions": "7.0.1"
  },
  "scripts": {
    "patch": "versions -C patch *.user.css",
    "minor": "versions -C minor *.user.css",
    "major": "versions -C major *.user.css",
    "update": "updates -cu && npm install"
  }
}
